//demo
pipeline {
    agent any 
    options { skipDefaultCheckout() }
    stages {
        stage('Build') { 
            steps {
                echo "Build"
            }
        }
        stage('Test') { 
            steps {
               echo "Test"
            }
        }
        stage('Deploy') { 
            steps {
               echo "Deploy" 
            }
        }
    }
}

